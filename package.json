{
  "name": "awesome-boilerplate-sass-tailwind-parcel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "rm -rf dist && parcel build src/index.html --no-source-maps"
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      "images": [
        ".jpg",
        ".png"
      ],
      ".css": "css"
    },
    "options": {
      "development": true
    }
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-custom-dist-structure": "^1.1.14",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "tailwindcss": "^1.6.2"
  }
}